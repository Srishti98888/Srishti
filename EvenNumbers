<script>
function get even numbers() {
var arr=[1,2,3,4,5,6];
for (var i=o;i<arr.length;i++) {
if(arr[i] % 2 === 0){
document.writeln(arr[i] + "<br/>");
}
}
}
get EvenNumbers();

</script>
